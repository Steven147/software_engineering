<!--pages/wordTest/wordTest.wxml-->
<view wx:for='{{word}}' wx:key="word">
<view class='container'>
<text class='word'> {{item.yisi}}</text>
<view>
  <button catchtap="true" bindtap="test.right">{{item.word}}</button>
  <button catchtap="wrong" bindtap="test.right" >wrong</button>
  <button catchtap="wrong" bindtap="test.right">wrong</button>
  <button catchtap="wrong" bindtap="test.right">wrong</button>
</view>
</view>
</view>
